<?xml version="1.0" encoding="UTF-8"?>
<!-- Do not edit this file, it will be overwritten on install.
        Copy the file to $HOME/.config/openbox/ instead. -->
<openbox_config xmlns="http://openbox.org/3.4/rc">
<keyboard>
<keybind key="*">
<action name="If">
<title>*</title>
<then></then>
<else></else>
</action>
</keybind>
</keyboard>
<applications>
<application class="*">
<decor>no</decor>
<position>
<x>center</x>
<y>center</y>
</position>
</application>
</applications>
</openbox_config>
